{
  "name": "Tabs 2 Links",
  "description": "Get the urls of all the current open tabs in a list",
  "version": "{{VERSION}}",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs",
    "notifications"
  ],
  "background": {
    "service_worker": "background/worker.js"
  },
  "action": {
    "default_popup": "action/index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
