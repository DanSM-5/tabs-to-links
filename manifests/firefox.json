{
  "name": "Tabs 2 Links",
  "description": "Get the urls of all the current open tabs in a list",
  "version": "{{VERSION}}",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "notifications"
  ],
  "browser_action": {
    "default_popup": "action/index.html",
    "browser_style": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "dan@sm.com"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
