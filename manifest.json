{
  "name": "Tabs 2 Links",
  "description": "Get the urls of all the current open tabs in a list",
  "version": "1.0.4",
  "manifest_version": 2,
  "homepage_url": "https://github.com/DanSM-5/tabs-to-links",
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "action/index.html",
    "browser_style": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "dan@sm.com"
    }
  }
}